#unosimo broj od 1 do 100
i = int(input("Unesi broj od 1 do 100: "))
# definiramo raspon brojeva
for i in range(1, 101):
    print(i)
    #definiramo broj koji je djeljiv sa 3 i printamo fizz
    if i % 3 == 0:
    print("fizz")
        #   #definiramo broj koji je djeljiv sa 5 i printamo buzz
        if i % 5 == 0:
    print("buzz")
            #   #definiramo broj koji je djeljiv sa 3 i 5 i printamo fizzbuzz
   if i % 3 == 0 and i % 5 == 0:
                print("fizzbuzz")
